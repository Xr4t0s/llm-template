    background: linear-gradient(90deg, #3f51b5, #7cfc00);\\n    text-align: center;\\n    color: #fff;\\n    padding: 3rem 1rem;\\n  }\\n  .final-cta h2 {\\n    color: #fff;\\n    margin-bottom: 1rem;\\n  }\\n  /* Footer */\\n  footer {\\n    background: #212121;\\n    color: #ccc;\\n    padding: 2rem 1rem;\\n    font-size: 0.9rem;\\n    text-align: center;\\n  }\\n  footer a {\\n    color: #7cfc00;\\n  }\\n  footer a:hover, footer a:focus {\\n    color: #a8ff48;\\n  }\\n  /* Responsive Nav */\\n  @media (max-width: 768px) {\\n    .nav-links {\\n      position: absolute;\\n      top: 58px;\\n      right: 0;\\n      background: #3f51b5;\\n      flex-direction: column;\\n      width: 200px;\\n      transform: translateX(100%);\\n      transition: transform 0.3s ease;\\n      border-bottom-left-radius: 8px;\\n      border-bottom-right-radius: 8px;\\n    }\\n    .nav-links.open {\\n      transform: translateX(0);\\n    }\\n    .nav-links a {\\n      padding: 1rem;\\n      border-bottom: 1px solid #5f6fc4;\\n    }\\n    .menu-button {\\n      display: block;\\n    }\\n  }\\n</style>\\n</head>\\n<body>\\n<!-- Header / Navigation -->\\n<header>\\n  <nav class=\\\"container\\\" role=\\\"navigation\\\" aria-label=\\\"Primary\\\">\\n    <a href=\\\"#\\\" class=\\\"logo\\\" aria-label=\\\"LLM Studio Home\\\">LLM Studio</a>\\n    <button class=\\\"menu-button\\\" aria-expanded=\\\"false\\\" aria-controls=\\\"primary-menu\\\" aria-label=\\\"Toggle navigation menu\\\">&#9776;</button>\\n    <div class=\\\"nav-links\\\" id=\\\"primary-menu\\\">\\n      <a href=\\\"#problem\\\">Problem</a>\\n      <a href=\\\"#solution\\\">Solution</a>\\n      <a href=\\\"#features\\\">Features</a>\\n      <a href=\\\"#how-it-works\\\">How It Works</a>\\n      <a href=\\\"#social-proof\\\">Community</a>\\n      <a href=\\\"#final-cta\\\">Get Started</a>\\n    </div>\\n  </nav>\\n</header>\\n\\n<!-- Hero Section -->\\n<section class=\\\"hero\\\" aria-label=\\\"Introduction and main call to action\\\